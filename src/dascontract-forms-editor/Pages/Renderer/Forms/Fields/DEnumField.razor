

@if (field.Label != null && field.Label != "") {
    <span>@field.Label</span><br>
}
<MatRadioGroup Items="@field.Data" Outlined="true" Style="min-width: 100%"></MatRadioGroup>

@code {
    [CascadingParameter]
    Form form { get; set; }
    [CascadingParameter]
    FieldGroup group { get; set; }
    [CascadingParameter]
    EnumField field { get; set; }
}